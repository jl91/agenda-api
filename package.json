{
  "name": "ts-node-skeleton",
  "version": "0.0.1",
  "description": "A simple typescript skeleton to work with node and express",
  "main": "index.js",
  "dependencies": {
    "express": "^4.16.2",
    "nodemon": "^1.14.8",
    "tslint": "^5.8.0",
    "typescript": "^2.6.2"
  },
  "devDependencies": {
    "concurrently": "^3.5.1"
  },
  "scripts": {
    "compile": "tsc -w --outDir compiled",
    "serve": "nodemon ./compiled/index.js",
    "start": "concurrently \"npm run compile\""
  },
  "keywords": [
    "ts",
    "typescript",
    "node",
    "nodejs",
    "nodets",
    "skeleton"
  ],
  "author": "John Lennon de Melo Fernandes <jlfernandes91@hotmail.com>",
  "license": "LGPL-3.0"
}
